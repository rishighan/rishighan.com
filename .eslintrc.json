{
  "extends": [
    "airbnb",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 10
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".jsx"
        ],
        "paths": [
          "./src"
        ]
      }
    }
  },
  "rules": {
    "no-underscore-dangle": 0,
    "id-length": 0,
    "indent": [
      1,
      4
    ],
    "no-console": 0,
    "no-unused-vars": [
      1,
      {
        "vars": "local",
        "args": "none"
      }
    ],
    "strict": 0
  },
  "env": {
    "browser": true
  }
}