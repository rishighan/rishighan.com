{
    "extends": ["airbnb-base", "plugin:react/recommended"],
    "parser": "babel-eslint",
    "parserOptions": {"ecmaVersion": 10},
    "settings": {
        "import/resolver": {
          "node": {
            "extensions": [".js", ".jsx"],
            "paths": ["./src"]
          }
        }
    },
    "rules": {
      "no-underscore-dangle": 0
    },
    "env": {
      "browser": true
    }
}